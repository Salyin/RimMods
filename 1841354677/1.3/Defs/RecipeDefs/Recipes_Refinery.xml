<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<RecipeDef>
		<defName>AB_PropaneToChemfuel</defName>
		<label>make chemfuel from propane</label>
		<description>Make a batch of chemfuel from propane. This is a very inefficient method of getting chemfuel, but useful to get rid of unwanted propane.</description>
		<jobString>Refining chemfuel from wax.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>2500</workAmount>
		<recipeUsers>
				<li>BiofuelRefinery</li>	  
		</recipeUsers>	
		 <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
				  <thingDefs>
					<li>AB_Propane</li>
				  </thingDefs>
				</filter>
				<count>75</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AB_Propane</li>
				
			</thingDefs>
		</fixedIngredientFilter>
		
		<products>
			<Chemfuel>35</Chemfuel>
		</products>
	</RecipeDef>
	



</Defs>
