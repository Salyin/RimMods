<?xml version="1.0" encoding="utf-8" ?>
<Patch>	

	<!-- Adds a Blocking SkyMind Connection comp to Dementia and Alzheimers -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName = "Dementia" or defName = "Alzheimers"]/comps</xpath>
        <value>
            <li Class="ATReforged.HediffCompProperties_SkyMindEffecter">
                <blocksConnection>true</blocksConnection>
            </li>
        </value>
	</Operation>

</Patch>